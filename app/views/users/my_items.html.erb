<%=render "partials/categories-list"%>
<div class="container main-content">
<h3>My Items Listed for Hire</h3>
<% if @items.length >0%>
 <div class="card-deck">
  <%@items.each do |item| %>
   <%=link_to "Edit",edit_item_path(item)%>
   <%=link_to "delete",item_path(book), method: :delete, data: { confirm: "Are you sure?" }%>
   <%=render "partials/item-card" , item: item%>   
  <%end%>
 </div>
 <div class="center-element">
  <%== pagy_bootstrap_nav(@pagy) %>
 </div>
<%end%>
</div>