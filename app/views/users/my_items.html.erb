<%=render "partials/categories-list"%>
<div class="container main-content">
<% if @items.length>0 %>
  <h3>My Items Listed for Hire</h3>
  <div class="card-deck">
    <%@items.each do |item| %>
   
     <%=render "items/item-card" , item: item%>
    <%end%> 
  </div>
  <div class="center-element">
    <%== pagy_bootstrap_nav(@pagy) %>
  </div> 
<%else%> 
   <h3>You have not yet listed any items for hire !!!</h3>
<%end%>
</div>